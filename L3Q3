import java.util.Scanner;
public class q3 {
	public static void main (String [] args) {
		Scanner s = new Scanner (System.in);
		int num;
		do {
			System.out.println("Digite um numero entre 1 e 100:");
			num = s.nextInt();
			if (num < 1 || num > 100) {
				System.out.println("Digite um numero entre 1 e 100:");
				num = s.nextInt();
			}
		}while(num < 1 || num > 100);
	}

}
